java_library(
  name='lib',
  deps=[
    '//lib:openapi',
  ],
  visibility=['PUBLIC'],
)

java_binary(
  name='plugin',
  meta_inf_directory='resources/META-INF',
  deps=[
    ':lib',
    '//lib:openapi',
  ],
  labels = [
    "intellij_plugin",
  ],
)

# Other executables with meta_inf_directories shouldn't overwrite the plugin's meta_inf
java_binary(
  name='executable',
  meta_inf_directory='resources',
  main_class='HelloWorld',
)
