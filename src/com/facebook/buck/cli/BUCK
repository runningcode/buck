genrule(
    name = "main-fixed",
    srcs = [
        ":main",
    ],
    out = "main-fixed.jar",
    cmd = " ".join([
        "$(exe //py/buck/zip:munger)",
        "--input=$(location :main)",
        "--output=$OUT",
        "--exclude=META-INF/MANIFEST.MF",
    ]),
    visibility = [
        "PUBLIC",
    ],
)
